<?xml version="1.0" encoding="utf-8"?>  
<resources>  
    <declare-styleable name="HomeButton">  
        <attr name="backcolor" format="integer" />  
        <attr name="textSize" format="dimension" />  
        <attr name="big" format="boolean" />  
        <attr name="home" format="integer" />  
        <attr name="text" format="string" />  
    </declare-styleable>  
    
    
        <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
</resources>  